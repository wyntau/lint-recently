{
  "name": "lint-recently",
  "version": "0.0.1",
  "description": "lint recently git commited files",
  "main": "./dist/lib/index.js",
  "bin": "./dist/bin/lint-recently.js",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:wyntau/lint-recently.git",
  "author": "wyntau <wyntau@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/debug": "^4.1.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.10.3",
    "@types/normalize-path": "^3.0.0",
    "@types/supports-color": "^8.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "cli-truncate": "2.x",
    "commander": "^8.2.0",
    "cosmiconfig": "^7.0.1",
    "dayjs": "^1.10.7",
    "debug": "^4.3.2",
    "execa": "^5.1.1",
    "normalize-path": "^3.0.0",
    "please-upgrade-node": "^3.2.0",
    "stringify-object": "3.x",
    "supports-color": "8.x",
    "tslib": "^2.3.1"
  },
  "scripts": {
    "eslint": "DEBUG=eslint:cli-engine eslint .",
    "eslint-fix": "DEBUG=eslint:cli-engine eslint --fix .",
    "prepare": "husky install || exit 0",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run prepublishOnly && npx . --debug"
  },
  "engines": {
    "npm": "use-yarn-please"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
