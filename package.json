{
  "name": "lint-recently",
  "version": "0.1.4",
  "description": "lint recently git commited files",
  "main": "./dist/index.js",
  "bin": "./dist/bin.js",
  "files": [
    "/dist"
  ],
  "repository": "git@github.com:wyntau/lint-recently.git",
  "author": "wyntau <wyntau@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/debug": "^4.1.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.10.3",
    "@types/normalize-path": "^3.0.0",
    "@types/supports-color": "^8.1.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.4.1",
    "type-fest": "^2.5.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "cli-truncate": "2.x",
    "colorette": "^2.0.16",
    "commander": "^9.0.0",
    "cosmiconfig": "^7.0.1",
    "dayjs": "^1.10.7",
    "debug": "^4.3.2",
    "execa": "^5.1.1",
    "listr2": "^4.0.0",
    "micromatch": "^4.0.4",
    "normalize-path": "^3.0.0",
    "p-map": "4.x",
    "please-upgrade-node": "^3.2.0",
    "string-argv": "^0.3.1",
    "supports-color": "8.x",
    "tslib": "^2.3.1"
  },
  "scripts": {
    "eslint": "DEBUG=eslint:cli-engine eslint .",
    "eslint-fix": "DEBUG=eslint:cli-engine eslint --fix .",
    "prepare": "husky install || exit 0",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "npm run eslint",
    "test": "npm run prepublishOnly && npx . --debug"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
